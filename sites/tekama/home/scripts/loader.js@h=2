 // matrix
 jQuery(document).ready(function() {
    var audio = new Audio();
    audio.src = '/home/sounds/matrix2.mp3';
    audio.volume = 0.8;
     jQuery(".matrixbtn").hover(function() {
        audio.play();
         jQuery(".matrix").stop().animate({
             opacity: 0.95
         }, 800);
         jQuery(".large-header").stop().animate({
             opacity: 0
         }, 5000);
     }, function() {
      audio.pause();
         jQuery(".matrix").stop().animate({
             opacity: 0
         }, 800);
         jQuery(".large-header").stop().animate({
             opacity: 1
         }, 3000);
     });
 });

 // Analizer
//îáðàáîò÷èê onsubmit ôîðìû
formSubmit = function (obj)
{
    var formData = new FormData (document.getElementById ('formsubmit'));
	$.ajax({
		url : '/tools/core/roboscan.php',
		type : 'POST',
		data : formData,
		processData: false,
		contentType: false,
		success : function (data) {
		    $(".resp_parser").hide();
			$("#results").html(data);
			$('#limit_num').load('/parser/parser_num.php');
		},
		beforeSend: function () {
		        $(".resp_parser").show();
	            $('#results').html ('');
	        }
	});
 document.getElementById ('formsubmit').reset ();
	return false;
}

